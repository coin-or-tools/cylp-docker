FROM ubuntu:latest
RUN apt-get update
RUN apt-get install -y python3 python3-pip wget unzip file
RUN apt-get install -y libblas-dev liblapack-dev
RUN wget https://www.coin-or.org/download/source/Cbc/Cbc-2.10.3.zip
RUN unzip Cbc-2.10.3.zip
WORKDIR Cbc-2.10.3
RUN ./configure --prefix=/var
RUN make
RUN make install
ENV COIN_INSTALL_DIR /var
RUN pip3 install numpy
RUN pip3 install --pre cylp
